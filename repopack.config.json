{
  "output": {
    "filePath": "repopack-output.txt",
    "style": "plain",
    "removeComments": false,
    "removeEmptyLines": false,
    "topFilesLength": 5,
    "showLineNumbers": false
  },
  "include": ["src/**", "cmake/**", "docs/**"],
  "ignore": {
    "useGitignore": true,
    "useDefaultPatterns": true,
    "customPatterns": ["src/**/*.xml", "src/test/**", "cmake/doxygen/**","src/scripts/**","src/main/matlab/**"]
  },
  "security": {
    "enableSecurityCheck": false
  }
}
